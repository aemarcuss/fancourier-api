{
    "name": "shusaura85/fancourier-api",
    "description": "Library for FanCourier API v2.0",
	"type": "library",
    "authors": [
        {
            "name": "Shu Saura",
            "role": "Developer"
        }
    ],
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "ext-curl": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4"
    },
    "autoload": {
        "psr-4": {
            "Fancourier\\": "src/Fancourier/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fancourier\\Tests\\": "tests/"
        }
    }
}
